//INSERT INTO Cursinho.contas_A_pagar  (idcontas_A_pagar,valorTotal,qtdParcelas,aluno_idaluno) VALUES ();
console.log("#Tabela contas_A_pagar")
contas.innerHTML="#Tabela contas_A_pagar<br>"
	pm=199.99
	vestibular=499.99
	bombeiro=199.99
	valor=[pm,vestibular,bombeiro];
	alunoXcurso=[3,2,1,1,2,3,2,1,1,2,3,3,2,1,1,2,3,3,2,1]

	parcelas=[]

	for(id=1;id<=20;id++){
		parcelas.push(Math.floor(Math.random()*10)%12+1);
		console.log("INSERT INTO Cursinho.contas_A_pagar  (idcontas_A_pagar,valorTotal,qtdParcelas,aluno_idaluno) VALUES ("+id+","+valor[alunoXcurso[id-1]-1]+","+parcelas[id-1]+","+id+");")
		contas.innerHTML+="INSERT INTO Cursinho.contas_A_pagar  (idcontas_A_pagar,valorTotal,qtdParcelas,aluno_idaluno) VALUES ("+id+","+valor[alunoXcurso[id-1]-1]+","+parcelas[id-1]+","+id+");"+"<br>";
	}
	contas.innerHTML+="<br>"

//INSERT INTO Cursinho.quitada  (idpagamento,contas_A_pagar_idcontas_A_pagar,valor,data,tipo_Pagamento) VALUES ();
console.log("#Tabela quitada")
quitadas.innerHTML="#Tabela quitada<br>"
	//parcelas veio de contas a pagar
	TipoPagamento=["Dinheiro","Debito","Credito","Cheque"];
	contador=1;
	for(i=0;i<parcelas.length;i++){
		for(j=1;j<=parcelas[i];j++){
			//Data de pagamento parcela ANO-MES-DIA
			data=""+2018+"-";
			if(parcelas[i]+1-j<10){
				data+="0"
			}
			data+=parcelas[i]+1-j;
			data+="-06";

			nTipo=Math.floor((Math.random()*1000)%4);
			console.log("INSERT INTO Cursinho.quitada  (idpagamento,contas_A_pagar_idcontas_A_pagar,valor,data,tipo_Pagamento) VALUES ("+contador+","+(i+1)+","+(valor[alunoXcurso[i]-1]/parcelas[i]).toFixed(2)+","+data+",'"+TipoPagamento[nTipo]+"');")
			quitadas.innerHTML+="INSERT INTO Cursinho.quitada  (idpagamento,contas_A_pagar_idcontas_A_pagar,valor,data,tipo_Pagamento) VALUES ("+contador+","+(i+1)+","+(valor[alunoXcurso[i]-1]/parcelas[i]).toFixed(2)+","+data+",'"+TipoPagamento[nTipo]+"');"+"<br>";
			contador++
		}
	}







//Insere na pagina
document.body.innerHTML=""
